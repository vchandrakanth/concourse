{
  "name": "concourse-app",
  "version": "0.0.0",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "file": false,
    "system": false
  },
  "scripts": {
    "ng": "ng",
    "start": "yarn start:poll",
    "start:normal": "ng serve --disable-host-check",
    "start:poll": "ng serve --poll=2000 --disable-host-check",
    "start:docs": "compodoc -s -d ./documentation/",
    "build": "ng build",
    "build:docs": "compodoc -p src/tsconfig.app.json --unitTestCoverage coverage/coverage-summary.json",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:ci": "ng test --runInBand",
    "test:coverage": "ng test --coverage",
    "test:update-snapshots": "ng test -u",
    "test:update-snapshot": "ng test -u -t",
    "lint": "ng lint",
    "e2e:local": "ng e2e",
    "build:e2e": "tsc --project e2e",

    "pretest": "npm run tsc",
    "test0": "protractor ./build/conf/conf.js --params.env=adhoc --chromeDriver=./src/conf/chromedriver.exe",
    "test1": "protractor ./build/conf/conf.js --params.env=beta --chromeDriver=./src/conf/chromedriver.exe",
    "test2": "protractor ./build/conf/conf.js --params.env=prod --chromeDriver=./src/conf/chromedriver.exe",
    "tsc": "tsc",
    "test-local": "env=local npm run test-local",
    "e2e:adhoc": "npm run test0",
    "e2e:beta": "npm run test1",
    "e2e:prod": "npm run test2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ctrl/ngx-codemirror": "^1.3.10",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.4.2",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@ng-select/ng-select": "^2.20.0",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/entity": "^7.4.0",
    "@ngrx/router-store": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@swimlane/ngx-datatable": "^14.0.0",
    "bootstrap": "^4.1.3",
    "codemirror": "^5.47.0",
    "core-js": "^2.5.4",
    "diff": "^4.0.1",
    "faker": "^4.1.0",
    "jshint": "^2.10.2",
    "jsonlint": "^1.6.3",
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.2",
    "ngx-bootstrap": "^5.0.0",
    "ngx-cacheable": "^1.0.9",
    "ngx-cookie-service": "^2.0.0",
    "ngx-pipes": "^2.4.5",
    "ngx-toastr": "^10.0.0",
    "panzoom": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.0.0",
    "@angular-builders/jest": "^7.0.0",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@compodoc/compodoc": "^1.1.7",
    "@ngrx/schematics": "^7.4.0",
    "@swimlane/ngx-charts": "^12.0.1",
    "@types/faker": "^4.1.4",
    "@types/jasmine": "^2.8.11",
    "@types/jest": "^24.0.15",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "~8.9.4",
    "angular-tslint-rules": "^1.5.0",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "^5.0.0",
    "jasmine-marbles": "^0.5.0",
    "jasmine-slack-reporter": "^1.4.0",
    "jest": "^24.8.0",
    "log4js-protractor-appender": "^1.1.2",
    "protractor": "~5.4.0",
    "protractor-screenshoter-plugin": "^0.10.3",
    "protractor-slack-plugin": "0.0.1",
    "serve": "^10.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.2.4",
    "vrsource-tslint-rules": "^6.0.0",
    "webpack-stats-plugin": "^0.3.0"
  }
}